# Path to the Miniconda installation
CONDA_PATH=C:/Users/glod/miniconda3

# Conda environment name
CONDA_ENV_NAME=ComfyUI

# Directory for ComfyUI installation (default is in user's home directory)
COMFYUI_DIR=C:/Users/glod/ComfyUI

# Directory paths (set USE_CUSTOM_PATHS to "true" to enable these)
USE_CUSTOM_PATHS=true
MODEL_BASE_PATH=T:/models
INPUT_DIR=D:/temp/ComfyUI/input
OUTPUT_DIR=D:/temp/ComfyUI/output
TEMP_DIR=D:/temp/ComfyUI/temp

# Ports and network settings
SERVER_PORT=8188

# Monitoring intervals (in seconds)
MONITOR_INTERVAL=10
BOOT_WAIT_TIME=30

# Logging and debugging options
ENABLE_LOGGING=true
LOG_FILE=ComfyUI_log.txt
